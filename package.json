{
  "name": "markdown-to-notion",
  "version": "1.0.0",
  "description": ".mdファイルの内容を、Notionに出力できるMCPサーバー",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "md-to-notion": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/camoneart/md-to-notion"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "packageManager": "pnpm@10.15.0",
  "keywords": [
    "mcp",
    "markdown",
    "notion",
    "server"
  ],
  "author": "camoneart",
  "homepage": "https://github.com/camoneart/md-to-notion",
  "bugs": {
    "url": "https://github.com/camoneart/md-to-notion/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.17.4",
    "@notionhq/client": "4.0.2",
    "@tryfabric/martian": "1.2.4",
    "dotenv": "17.2.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@types/jest": "30.0.0",
    "@types/node": "24.3.0",
    "@typescript-eslint/eslint-plugin": "8.40.0",
    "@typescript-eslint/parser": "8.40.0",
    "eslint": "9.34.0",
    "jest": "30.0.5",
    "ts-jest": "29.4.1",
    "tsx": "4.20.4",
    "typescript": "5.9.2"
  }
}
